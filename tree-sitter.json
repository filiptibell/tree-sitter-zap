{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "zap",
      "camelcase": "Zap",
      "title": "Zap",
      "scope": "source.zap",
      "file-types": ["zap"],
      "injection-regex": "^zap$",
      "class-name": "TreeSitterZap"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Zap grammar for tree-sitter",
    "authors": [
      {
        "name": "Filip Tibell"
      }
    ],
    "links": {
      "repository": "https://github.com/filiptibell/tree-sitter-zap"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
